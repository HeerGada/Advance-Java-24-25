<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Page1Servlet</servlet-name>
        <servlet-class>Page1Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Page2Servlet</servlet-name>
        <servlet-class>Page2Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Page3Servlet</servlet-name>
        <servlet-class>Page3Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Page4Servlet</servlet-name>
        <servlet-class>Page4Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Page5Servlet</servlet-name>
        <servlet-class>Page5Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Page1Servlet</servlet-name>
        <url-pattern>/Page1Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Page2Servlet</servlet-name>
        <url-pattern>/Page2Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Page3Servlet</servlet-name>
        <url-pattern>/Page3Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Page4Servlet</servlet-name>
        <url-pattern>/Page4Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Page5Servlet</servlet-name>
        <url-pattern>/Page5Servlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
